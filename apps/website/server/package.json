    "@types/ioredis": "^4.28.10",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
