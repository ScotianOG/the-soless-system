{
  "name": "soless-server",
  "version": "1.0.0",
  "description": "SOLess Backend Server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:worker": "node dist/worker.js",
    "start:bot": "node dist/core/bots/index.js",
    "start:social-ai": "node dist/core/social-ai/startSocialAI.js",
    "start:social-ai:standalone": "node dist/core/social-ai/startSocialAI.js standalone",
    "social-ai": "npm run build && npm run start:social-ai",
    "social-ai:standalone": "npm run build && npm run start:social-ai:standalone",
    "social-ai:dev": "ts-node src/core/social-ai/startSocialAI.ts",
    "social-ai:setup": "psql -d $DATABASE_URL -f src/core/social-ai/schema.sql",
    "dev": "nodemon",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "jest src/tests/production-readiness.ts --forceExit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
