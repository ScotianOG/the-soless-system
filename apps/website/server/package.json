    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "prisma": "^6.12.0",
    "tweetnacl": "^1.0.3",
